{
  "name": "gtts.js",
  "version": "1.0.1",
  "description": "A Promise based Node.js/TypeScript port of the gTTS python library",
  "main": "dist/gtts.js",
  "types": "dist/gtts.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hubendubler/gTTS.js.git"
  },
  "bugs": {
    "url": "https://github.com/hubendubler/gTTS.js/issues"
  },
  "homepage": "https://github.com/hubendubler/gTTS.js#readme",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/playground/"
    ],
    "testEnvironment": "node"
  },
  "keywords": [
    "gtts",
    "text-to-speech",
    "node-module",
    "typescript",
    "nodejs",
    "tts",
    "speech"
  ],
  "author": "hubendubler",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "jest": "^23.6.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@types/jsdom": "^12.2.3",
    "axios": "^0.18.0",
    "jsdom": "^15.0.0"
  }
}
